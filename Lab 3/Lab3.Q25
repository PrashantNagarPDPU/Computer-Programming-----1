#include <stdio.h>

void main() 
{
    int n, num, rev = 0, rem;

    printf("Enter an integer: ");
    scanf("%d", &n);

    num = n;

    while(n!=0)
    {
        rem= n% 10; 
        rev= rev * 10 + rem;
        n/=10; 
    }

    if (num== rev) 
    {
        printf("%d is a palindrome number.\n", num);
    }
     else
      {
        printf("%d is not a palindrome number.\n", num);
    }
}
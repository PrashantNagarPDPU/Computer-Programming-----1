#include <stdio.h>

int main() 
{
    int num;
    int largest;
    int smallest;

    printf("Enter 100 numbers:\n");

    for (int i = 0; i < 100; i++)
     {
        printf("Enter number %d: ", i + 1);
        scanf("%d", &num);

        if (num > largest)
         {
            largest = num; 
        }

        if (num < smallest)
         {
            smallest = num;
        }
    }

    printf("\nLargest number: %d\n", largest);
    printf("Smallest number: %d\n", smallest);
}